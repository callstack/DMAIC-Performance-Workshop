{
  "name": "workshopapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "pods": "npx pod-install",
    "start": "conc -kr -p none \"npm run start:backend\" \"npm run start:client\"",
    "start:client": "react-native start --experimental-debugger",
    "start:backend": "node ./backend/start.js",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:prod": "react-native run-android --mode=release --no-packager && npm run start:backend",
    "ios:prod": "react-native run-ios --mode=Release --no-packager && npm run start:backend",
    "lint": "eslint .",
    "test": "jest",
    "bundle-visualizer:android": "npx react-native-bundle-visualizer --platform android --bundle-output ./android.bundle",
    "bundle-visualizer:ios": "npx react-native-bundle-visualizer --platform ios --bundle-output ./ios.bundle"
  },
  "dependencies": {
    "@op-engineering/op-sqlite": "^6.1.2",
    "@react-native-picker/picker": "^2.7.7",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@tanstack/react-query": "^5.17.9",
    "date-fns": "^3.6.0",
    "i18next": "^23.11.5",
    "libphonenumber-js": "^1.11.4",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-i18next": "^14.1.2",
    "react-native": "0.74.3",
    "react-native-blob-util": "^0.19.9",
    "react-native-safe-area-context": "^4.10.7",
    "react-native-screens": "^3.32.0",
    "react-native-svg": "^15.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@types/lodash": "^4.17.6",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-native-bundle-visualizer": "3.1.3",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=20"
  }
}
